{
  "name": "personal-site",
  "version": "1.1.1",
  "description": "Personal portfolio website built with React, Bootstrap, and Express.",
  "scripts": {
    "start": "npm run build && node src/server/index.js",
    "dev": "concurrently \"npm run dev-server\" \"npm run dev-client\" \"npm run sass:watch\"",
    "build": "webpack --mode production",
    "sass:compile": "node-sass src/client/sass/sass.scss -o src/client/css/sass.css",
    "sass:watch": "node-sass --watch src/client/sass/sass.scss -o src/client/css/",
    "dev-client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "dev-server": "nodemon src/server/index.js"
  },
  "author": "Nicholas Young",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "bootstrap": "^4.3.1",
    "core-js": "^3.2.1",
    "eslint-plugin-react-hooks": "^1.7.0",
    "express": "^4.17.1",
    "jquery": "^3.6.0",
    "node-sass": "^4.14.1",
    "nodemailer": "^6.6.1",
    "popper.js": "^1.15.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "sass": "^1.22.10",
    "vis": "^4.21.0",
    "wowjs": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.2",
    "css-loader": "^3.2.0",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "nodemon": "^1.19.2",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.11.2"
  }
}
