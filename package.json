{
  "name": "personal-site",
  "version": "0.0.1",
  "description": "Personal portfolio website built with React, Bootstrap, and Express.",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && node src/server/index.js",
    "sass:compile": "node-sass --watch src/client/sass/sass.scss -o src/client/css/",
    "dev-client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "dev-server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run dev-server\" \"npm run dev-client\" \"npm run sass:compile\""
  },
  "author": "Nicholas Young",
  "license": "ISC",
  "dependencies": {
    "@bit/bondz.react-epic-spinners.semipolar-spinner": "^0.3.0",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "4.3.1",
    "express": "^4.16.3",
    "jquery": "^3.3.1",
    "node-sass": "^4.11.0",
    "popper.js": "^1.14.7",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scrollable-anchor": "^0.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "css-loader": "^2.0.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.17.3",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  }
}
